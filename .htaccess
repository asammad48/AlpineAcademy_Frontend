RewriteEngine On

# Disable automatic directory slash handling
DirectorySlash Off

# ========================================
# 1. STATIC FILES - Serve directly
# ========================================
RewriteCond %{REQUEST_URI} \.(css|js|jpg|jpeg|png|gif|webp|svg|ico|woff|woff2|ttf|eot|pdf|zip|mp4|webm|txt|xml)$ [NC]
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# ========================================
# 2. EXACT FILE MATCHES - Serve directly
# ========================================
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# ========================================
# 3. DIRECTORY INDEX - Serve index.html
# ========================================
RewriteRule ^$ /index.html [L]
RewriteRule ^en/?$ /en/index.html [L]
RewriteRule ^fr/?$ /fr/index.html [L]
RewriteRule ^ca/?$ /ca/index.html [L]
RewriteRule ^pt/?$ /pt/index.html [L]

# Blog index pages - handle before blog articles
RewriteRule ^blog/?$ /blog.html [L]
RewriteRule ^en/blog/?$ /en/blog.html [L]
RewriteRule ^ca/blog/?$ /ca/blog.html [L]
RewriteRule ^fr/blog/?$ /fr/blog.html [L]
RewriteRule ^pt/blog/?$ /pt/blog.html [L]

# ========================================
# 4. BLOG ARTICLES - Cross-Language Redirects & Clean URLs
# ========================================

